{
  "name": "meetup",
  "displayName": "meetup",
  "description": "VSCode extension with live video/audio meeting and collaborative coding",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:meetup.openRecorder",
    "onCommand:webrtcCollab.start"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "meetup.openRecorder",
        "title": "Open Video/Audio Meet"
      },
      {
        "command": "webrtcCollab.start",
        "title": "Start Collaborative Coding (WebRTC + Yjs)"
      }
    ],
    "customEditors": [
      {
        "viewType": "meetup.recorder",
        "displayName": "Camera Recorder",
        "selector": [
          {
            "filenamePattern": "*.recorder"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.106.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1"
  },
  "dependencies": {
    "ws": "^8.18.3"
  }
}
